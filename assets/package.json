{
  "name": "woocommerce-sales-insights",
  "version": "1.0.0",
  "description": "A plugin for sending WooCommerce monthly sales reports via email.",
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf assets/build/*",
    "svg": "svgr -d assets/src/js/icons assets/src/icons",
    "lint:php": "vendor/bin/phpcs",
    "lint:php:fix": "vendor/bin/phpcbf",
    "lint": "eslint assets/src/js/*",
    "lint:fix": "eslint --fix assets/src/js/*",
    "lint-css": "stylelint 'assets/src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint:php && npm run lint-css:fix && npm run lint:fix && npm run prod",
    "cypress:open": "node_modules/.bin/cypress open"
  },
  "keywords": [
    "wordpress",
    "WooCommerce"
  ],
  "author": "Win Authority LLC",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@svgr/cli": "^8.1.0",
    "@wordpress/base-styles": "^4.41.0",
    "@wordpress/block-editor": "^12.18.2",
    "@wordpress/blocks": "^12.27.1",
    "@wordpress/components": "^25.16.0",
    "@wordpress/compose": "^6.27.0",
    "@wordpress/data": "^9.20.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^5.1.0",
    "@wordpress/element": "^5.27.0",
    "@wordpress/eslint-plugin": "^17.7.0",
    "@wordpress/hooks": "^3.50.0",
    "@wordpress/i18n": "^4.50.0",
    "@wordpress/icons": "^9.41.0",
    "@wordpress/server-side-render": "^4.27.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^48.0.4",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.7",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "sass-loader": "^14.1.0",
    "sass-mq": "^6.0.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "defaults"
  ]
}
